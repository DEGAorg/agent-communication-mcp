{
  "packageManager": "yarn@4.1.0",
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && tsc && mkdir -p dist/zk/proofs && cp -r src/zk/proofs/* dist/zk/proofs/",
    "start": "node --experimental-specifier-resolution=node dist/server.js",
    "dev": "tsx src/server.ts",
    "dev:stdio": "tsx src/stdio-server.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:tools": "NODE_OPTIONS=--experimental-vm-modules jest test/tools.test.ts",
    "test:encryption": "NODE_OPTIONS=--experimental-vm-modules jest test/encryption.test.ts",
    "test:file": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "auth:setup": "tsx src/cli/auth.ts setup",
    "auth:check": "tsx src/cli/auth.ts check",
    "auth:retry": "tsx src/cli/auth.ts retry",
    "service:register": "tsx src/cli/service.ts",
    "service:register:test": "tsx src/cli/service.ts --test",
    "keys:generate": "tsx src/cli/keys.ts generate",
    "setup:agent": "tsx scripts/setup-agent.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@noble/ciphers": "^1.3.0",
    "@noble/curves": "^1.9.0",
    "@noble/hashes": "^1.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "@types/inquirer": "^9.0.8",
    "chalk": "^5.3.0",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "fp-ts": "^2.16.1",
    "inquirer": "^12.6.1",
    "io-ts": "^2.2.20",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "rxjs": "^7.8.1",
    "snarkjs": "^0.7.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/circomlibjs": "^0",
    "@types/jest": "^29.5.6",
    "@types/node": "^22.15.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2"
  }
}
